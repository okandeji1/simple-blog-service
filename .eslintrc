{
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "warn",
    "import/prefer-default-export": "off",
    "import/extensions": [
      "never"
    ],
    "import/no-unresolved": "error"
  }
}